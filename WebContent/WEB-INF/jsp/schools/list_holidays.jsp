<div id="addHolidayModal" class="modal modal-fixed-footer addHolidayModal">
	<div class="modal-content">
		<div class="row form_view_row">
			<h5 class="Lato_normal_font">Add Holiday</h5>
		</div>
		<div class="row form_view_row">
			<div class="col s12 m12 l12 input-field">
				<input type="text" id="holidayName" class="validate" /> <label for="holidayName">Name</label>
			</div>
		</div>
		<div class="row form_view_row">
			<div class="col s12 m12 l12 input-field">
				<textarea id="holidayDesc" class="materialize-textarea"></textarea>
				<label for="holidayDesc">Description</label>
			</div>
		</div>
		<div class="row form_view_row">
			<div class="col s6 m4 l4">
				<input name="group1" type="radio" id="holiPublic" checked /> <label for="holiPublic">Public
					Holiday</label>
			</div>
			<div class="col s6 m4 l4">
				<input name="group1" type="radio" id="holiPublic" checked /> <label for="holiPublic">School
					Holiday</label>
			</div>
			<div class="col s6 m4 l4">
				<select>
					<option value="" disabled selected>Select Term</option>
					<option value="1">1</option>
					<option value="2">2</option>
				</select>
			</div>
		</div>
		<div class="row form_view_row">
			<div class="col s4 m2 l2 input-field">
				<h6 class="Lato_normal_font">Date:</h6>
			</div>
			<div class="col s4 m4 l4 input-field">
				<input type="date" class="datepicker" placeholder="Start date" />
			</div>
			<div class="col s4 m4 l4 input-field">
				<input type="date" class="datepicker" placeholder="End date" />
			</div>
		</div>
		<div class="row form_view_row">
			<div class="col s12 m12 l12 input-field">
				<input type="checkbox" class="filled-in" id="holidayAppli" /> <label for="holidayAppli">Not
					Applicable to School</label>
			</div>
		</div>
	</div>
	<div class="modal-footer">
		<a href="#!" class=" modal-action modal-close waves-effect waves-green btn-flat">Cancel</a> <a
			href="#!" class=" modal-action modal-close waves-effect waves-green btn-flat">Create</a>
